{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run build:web && pnpm --filter @portfolio/cdk run build",
    "build:web": "pnpm exec open-next build",
    "start": "next start",
    "lint": "pnpm exec tsc --noEmit && eslint .",
    "format": "prettier --write .",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:real-api": "PLAYWRIGHT_SKIP_WEBSERVER=true E2E_USE_REAL_APIS=true pnpm exec playwright test --project=real-integration",
    "test:real-api:dev": "E2E_USE_REAL_APIS=true pnpm exec playwright test --project=real-integration",
    "test:real-ui": "PLAYWRIGHT_SKIP_WEBSERVER=true E2E_USE_REAL_APIS=true pnpm exec playwright test --project=chromium",
    "test:real-ui:dev": "E2E_USE_REAL_APIS=true pnpm exec playwright test --project=chromium",
    "test:real-ui:dev:headed": "E2E_USE_REAL_APIS=true pnpm exec playwright test --project=chromium --headed",
    "test:projectSearch": "tsx tests/projectSearch.integration.ts",
    "lint:test-boundaries": "tsx scripts/check-test-boundaries.ts",
    "chat:preprocess": "tsx scripts/chat-preprocess.ts",
    "chat:preprocess:profile": "CHAT_PREPROCESS_TASK=profile tsx scripts/chat-preprocess.ts",
    "chat:evals": "tsx scripts/chat-evals.ts",
    "export:chat:unified": "tsx scripts/chat/export-chat-core.ts",
    "sync:local": "tsx scripts/sync-env-to-github.ts --env=.env.local --environment=dev",
    "sync:dev": "tsx scripts/sync-env-to-github.ts --env=.env.development --environment=dev",
    "sync:staging": "tsx scripts/sync-env-to-github.ts --env=.env.staging --environment=staging",
    "sync:prod": "pnpm run sync:prod:github && pnpm run sync:prod:aws",
    "sync:prod:github": "tsx scripts/sync-env-to-github.ts --env=.env.production --environment=production",
    "sync:prod:aws": "tsx scripts/sync-env-to-aws.ts --env=.env.production --environment=production"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.927.0",
    "@aws-sdk/client-cloudwatch": "^3.927.0",
    "@aws-sdk/client-dynamodb": "^3.927.0",
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/client-scheduler": "^3.927.0",
    "@aws-sdk/client-secrets-manager": "^3.637.0",
    "@aws-sdk/client-ses": "^3.637.0",
    "@aws-sdk/client-sns": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@octokit/rest": "^21.1.0",
    "@portfolio/github-data": "workspace:*",
    "@radix-ui/react-slot": "^1.1.0",
    "@smithy/config-resolver": "^4.4.3",
    "@smithy/fetch-http-handler": "^5.3.6",
    "@smithy/node-http-handler": "^4.4.3",
    "@tanstack/react-query": "^5.90.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.23.24",
    "geist": "^1.5.1",
    "hast-util-sanitize": "^5.0.2",
    "highlight.js": "^11.10.0",
    "js-tiktoken": "^1.0.21",
    "lucide-react": "^0.428.0",
    "minisearch": "^7.0.0",
    "next": "^15.1.6",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "rehype-highlight": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "simple-icons": "^13.6.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@next/env": "^15.1.6",
    "@next/eslint-plugin-next": "^15.1.6",
    "@opennextjs/aws": "^3.8.5",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@portfolio/test-support": "workspace:*",
    "autoprefixer": "^10.4.20",
    "esbuild": "0.17.16",
    "eslint": "^9.19.0",
    "libsodium-wrappers": "^0.7.15",
    "postcss": "^8.4.41",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
